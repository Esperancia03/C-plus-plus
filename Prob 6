#include <iostream>

using namespace std;

int main()
{
    int num;
    int remainder;
    int reversedNum =0;
    cin >> num;

    int temp = num;
    while(temp > 0)
    {
        remainder = temp % 10;
        reversedNum = (reversedNum * 10) + remainder;
        temp /=10;
    }
    
        
    cout << reversedNum << endl;
        
    


}

#include<iostream>
#include<cmath>

using namespace std;

class PrimeFactorization
{
    public:
    int n;
    void FindPrimeFactors( int n)
    {
        cout << n << " " << "=";
       
        while(n % 2 == 0)
        {
            bool first = true;
            if(!first)      
            
            cout << 2 << "*";
            n /=2;
            first =false;
        } 
        for (int i = 3; sqrt(i) >= i ; i +=2 )
        {
            while (n % i ==0)
            {
                if(!first)      
            
                cout << i << "*";
                n /=i;
                first =false;
        }
        if(n >=2)
        {
            if(!first) 
                       cout << n << endl;
        }
    }
};

int main()
{
    
PrimeFactorization P;
    for(int i=0; i<=1000; i++)
    {
        cin >> i;
        P.FindPrimeFactors(i);
    }

}
